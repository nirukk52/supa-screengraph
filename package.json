{
	"name": "supastarter-nextjs",
	"private": true,
	"scripts": {
		"build": "dotenv -c -- turbo build",
		"dev": "dotenv -c -- turbo dev --concurrency 15",
		"start": "dotenv -c -- turbo start",
		"lint": "biome lint . ",
		"check": "biome check",
		"clean": "turbo clean",
		"format": "biome format . --write",
		"prepare": "husky"
	},
	"engines": {
		"node": ">=20"
	},
	"packageManager": "pnpm@10.14.0",
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@repo/tsconfig": "workspace:*",
		"@types/node": "^24.7.2",
		"dotenv-cli": "^10.0.0",
		"husky": "^9.1.6",
		"turbo": "^2.5.8",
		"typescript": "5.9.3"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "19.0.0",
			"@types/react-dom": "19.0.0"
		}
	}
}
