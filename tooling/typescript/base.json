{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"allowJs": true,
		"composite": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"inlineSources": false,
		"isolatedModules": true,
		"module": "Preserve",
		"target": "ES6",
		"moduleResolution": "bundler",
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"preserveWatchOutput": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": true,
		"paths": {
			"@sg/agents-contracts": ["../../packages/agents-contracts/src"],
			"@sg/agents-contracts/*": ["../../packages/agents-contracts/src/*"],
			"@sg/eventbus": ["../../packages/eventbus/src"],
			"@sg/eventbus/*": ["../../packages/eventbus/src/*"],
			"@sg/eventbus-inmemory": ["../../packages/eventbus-inmemory/src"],
			"@sg/eventbus-inmemory/*": ["../../packages/eventbus-inmemory/src/*"],
			"@sg/queue": ["../../packages/queue/src"],
			"@sg/queue/*": ["../../packages/queue/src/*"],
			"@sg/queue-inmemory": ["../../packages/queue-inmemory/src"],
			"@sg/queue-inmemory/*": ["../../packages/queue-inmemory/src/*"],
			"@sg/feature-agents-run": [
				"../../packages/features/agents-run/src"
			],
			"@sg/feature-agents-run/*": [
				"../../packages/features/agents-run/src/*"
			],
			"@repo/api": ["../../packages/api"],
			"@repo/api/*": ["../../packages/api/*"]
		}
	},
	"display": "Default",
	"exclude": ["node_modules", "dist", ".next", "build"]
}
