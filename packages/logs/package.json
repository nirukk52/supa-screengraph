{
	"name": "@repo/logs",
	"version": "0.0.0",
	"dependencies": {
		"@repo/config": "workspace:*",
		"consola": "^3.4.2"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.7",
		"@repo/tsconfig": "workspace:*",
		"@types/node": "^24.9.1"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
