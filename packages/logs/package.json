		"./lib/*": {
			"import": "./dist/lib/*.js",
			"require": "./dist/lib/*.js",
			"types": "./dist/lib/*.d.ts"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "tsc -b",
		"type-check": "tsc --noEmit"
	}
}
